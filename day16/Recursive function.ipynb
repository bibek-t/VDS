   "source": [
    "def factorial(n):\n",
    "     if n == 0 :    #Base case factorial of 0 is 1\n",
    "        return 1\n",
    "    else:\n",
    "        return n *factorial(n-1)   #recursive case n!=n*(n-1)!\n",
    "    \n",
    "print (factorial(5))"
   ]
  },
