    "##  Limit of a function"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "fa77b501",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "2\n"
     ]
    }
   ],
   "source": [
    "import sympy as sp\n",
    "\n",
    "x= sp.symbols('x')\n",
    "\n",
    "f = (x**2 -1)/(x-1)\n",
    "\n",
    "limit =sp.limit(f,x,1)\n",
    "\n",
    "print(limit)"
   ]
  },
