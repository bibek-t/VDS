   ],
   "source": [
    "import sympy as sp\n",
    "\n",
    "x= sp.symbols('x')\n",
    "\n",
    "f =sp.sin(x)\n",
    "\n",
    "dfdx = sp.diff(f,x)\n",
    "\n",
    "print(dfdx)"
   ]
